{% extends 'base.html.twig' %}

{% block body %}
<div class="row">
{% for iKey, aKey in lstKeys %}
<div class="panel panel-default col-xs-6 col-md-3">
	<div class="panel-heading">{{ aKey.key }} <span class="badge">ttl:{{ aKey.ttl }}</span></div>
	<p class="lead">{{ aKey['value'] }}</p>
	<form action="" method="POST" id="oRedis_delete_{{ iKey }}">
		<input type="hidden" name="act" value="delete" />
		<input type="hidden" name="key" value="{{ aKey.key }}" />
		<span class="glyphicon glyphicon-trash btn btn-danger btn-xs" aria-hidden="true"
			onclick="$('#oRedis_delete_{{ iKey }}').submit();"
			style="float:right;"
			></span>
	</form>
</div>
{% endfor %}
</div>
{% endblock %}

